   "outputs": [],
   "source": [
    "function make_K(n: Int, alfa, beta, h)\n",
    "    K = zeros(n, n)\n",
    "    for i in 1:n-1\n",
    "        K[i, i] = 2*(alfa/h + beta*h/3)\n",
    "        K[i, i+1] = -alfa/h + beta*h/6\n",
    "        K[i+1, i] = -alfa/h + beta*h/6\n",
    "    end\n",
    "    K[n, n] = 2*(alfa/h + beta*h/3)\n",
    "end"
   ]
  },
